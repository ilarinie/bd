{
  "name": "bd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e .env -- ts-node-dev --respawn --transpile-only src/index.ts",
    "test:migrate": "dotenv -e .env.test -- prisma migrate dev",
    "test:watch": "dotenv -e .env.test -- vitest --coverage",
    "test": "dotenv -e .env.test -- vitest run --coverage",
    "start:prod": "prisma migrate deploy && prisma generate && node -r dotenv/config dist/index.js",
    "prepare": "husky install",
    "pretest": "npm run lint"
  },
  "author": "ilarinie",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "5.10.1",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "date-fns": "3.3.1",
    "dotenv": "16.4.5",
    "dotenv-cli": "7.3.0",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.2",
    "prisma": "5.10.1",
    "typescript": "5.3.3",
    "winston": "3.11.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/supertest": "6.0.2",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vitest/coverage-v8": "1.3.1",
    "eslint": "8.56.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "supertest": "6.3.4",
    "ts-node-dev": "2.0.0",
    "uuid": "9.0.1",
    "vitest": "1.3.1"
  }
}
